{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "dashboard#build": {
      "dependsOn": ["copyright#test", "copyright#typecheck"],
      "outputs": [".next/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": [""]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
